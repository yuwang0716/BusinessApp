<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_certification_business"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.liuhesan.app.businessapp.ui.personcenter.CertificationBusinessActivity">
    <android.support.v7.widget.Toolbar
        android:id="@+id/certificate_business_toolbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingTop="20dp"
        android:background="?attr/colorPrimary">
        <TextView
            android:text="营业资质"
            style="@style/toolbar_title"/>
    </android.support.v7.widget.Toolbar>
    <TextView
        android:id="@+id/business_license"
        android:layout_marginTop="16dp"
        style="@style/mine_style_layout"
        android:text="营业执照"
        android:drawableRight="@mipmap/icon_mine_details"/>
    <!--分割线-->
    <TextView
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/firstline_color"/>
    <TextView
        android:id="@+id/special_certification"
        style="@style/mine_style_layout"
        android:text="特殊资质"
        android:drawableRight="@mipmap/icon_mine_details"/>
    <!--分割线-->
    <TextView
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/firstline_color"/>
    <TextView
        android:id="@+id/corporate_certification"
        style="@style/mine_style_layout"
        android:text="法人身份证"
        android:drawableRight="@mipmap/icon_mine_details"/>

</LinearLayout>
